{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/_next/*", "/static/*", "/*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2}"]
  },
  "routes": [
    {
      "route": "/api/auth/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY"
  }
}
